{
  "name": "honey-beaz",
  "version": "1.0.0",
  "description": "Test Task Template, express + ejs",
  "main": "server.js",
  "author": "Jinxzzl <Azazel.dev@tuta.io>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "scss:build": "sass client/public/scss/:client/public/css/ --style=compressed",
    "server:build": "rm -rf ./server/build && tsc",
    "server:dev": "nodemon server/src/server.ts",
    "client:dev": "sass --watch client/public/scss/:client/public/css/ --style=compressed",
    "build": "npm run scss:build && npm run server:build",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "start": "cross-env NODE_ENV=production node server/build/server.js"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.6",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12",
    "sass": "^1.43.3",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "nodemon": "^2.0.14"
  }
}

<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body>
    <header class="header">
        <%- include('../partials/header'); %>
    </header>
    <main>
        <div class="section hot-products">
            <div class="container">
                <div class="row">
                    <div class="item">
                        <h1>Widget</h1>
                    </div>
                </div>
            </div>
        </div>
        <div class="section top-picks">
            <h1>Best Quality Product</h1>
            <% products.forEach( (product) => { %>
                <p><%= product.name %></p>
            <% }) %>
        </div>
    </main>
    <footer>
        <%- include('../partials/footer'); %>
    </footer>
</body>
</html>